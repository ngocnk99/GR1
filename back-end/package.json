        "morgan": "^1.9.1",
        "mssql": "^4.2.0",
        "multer": "^1.3.0",
        "mustache": "^3.0.1",
        "mysql2": "^1.6.5",
        "needle": "^2.3.0",
        "numeral": "^2.0.6",
        "pretty-error": "^2.1.1",
