        "request": "^2.83.0",
        "rich-filemanager-node": "^0.1.5",
        "sequelize": "^5.7.4",
        "sequelize-redis": "^1.0.12",
        "serve-favicon": "^2.5.0",
        "shelljs": "^0.8.3",
