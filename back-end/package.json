{
    "name": "cdn-maxping-cms-api",
    "version": "1.0.1",
    "description": "cms api for web",
    "scripts": {
        "start": "cross-env NODE_ENV=production nodemon dist/server.js -w dist/server.js -w .env.production",
        "build": "cross-env NODE_ENV=production webpack --config webpack.config.production.js",
        "dev": "nodemon --watch .env --watch src --exec babel-node src --source-maps",
        "lint": "eslint src test",
        "lint:fix": "eslint src test --fix",
        "codecov": "cat coverage/*/lcov.info | codecov",
        "seed": "knex seed:run --knexfile src/knexfile.js",
        "migrate": "knex migrate:latest --knexfile src/knexfile.js",
        "rollback": "knex migrate:rollback --knexfile src/knexfile.js",
        "prettier": "prettier --write **/*.{js,json,yml,yaml,md}",
        "make:seeder": "knex seed:make --knexfile src/knexfile.js",
        "make:migration": "knex migrate:make --knexfile src/knexfile.js",
        "test": "cross-env NODE_ENV=test mocha --exit --recursive --require @babel/register test",
        "test:coverage": "cross-env NODE_ENV=test nyc --require @babel/register --reporter lcov --reporter text mocha --exit --recursive test"
    },
    "lint-staged": {
        "*.js": [
            "eslint --fix",
            "prettier --write",
            "git add"
        ],
        "*.{md,yml,yml,json}": [
            "prettier --write",
            "git add"
        ]
    },
    "keywords": [
        "javascript",
        "node",
        "express",
        "es6",
        "api"
    ],
    "author": "phongnv86",
    "license": "MIT",
    "dependencies": {
        "@elastic/elasticsearch": "^7.0.0-rc.2",
        "@sentry/node": "^4.6.5",
        "async": "^2.6.0",
        "axios": "^0.19.0",
        "bluebird": "^3.5.4",
        "body-parser": "^1.18.1",
        "boom": "^7.3.0",
        "compression": "^1.7.4",
        "connect-multiparty": "^2.1.1",
        "cookie-parser": "^1.4.3",
        "cors": "^2.8.4",
        "debug": "^2.6.8",
        "dotenv": "^6.0.0",
        "ejs": "^2.5.7",
        "elastic-apm-node": "^2.11.5",
        "express": "^4.15.4",
        "express-jwt": "^5.3.1",
        "fs-extra": "^6.0.1",
        "helmet": "^3.16.0",
        "http-status-codes": "^1.3.2",
        "jimp": "^0.6.1",
        "joi": "^14.3.1",
        "jsonwebtoken": "^8.5.1",
        "knex": "^0.14.6",
        "lodash": "^4.17.11",
        "mobile-detect": "^1.4.3",
        "moment": "^2.22.2",
